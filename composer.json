{
    "name": "sickdaflip/mage2-customattributes",
    "description": "Adds virtual product attributes for price comparison feeds (Idealo, Billiger.de) including prices with tax and full URLs",
    "type": "magento2-module",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Philipp Breitsprecher",
            "email": "philippbreitsprecher@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "magento/framework": "^103.0",
        "magento/module-catalog": "^104.0",
        "magento/module-tax": "^100.4",
        "magento/module-store": "^101.1"
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "FlipDev\\CustomAttributes\\": "src/"
        }
    }
}
